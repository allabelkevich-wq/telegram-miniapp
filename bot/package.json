{
  "name": "yupsoul-bot",
  "version": "1.2.22",
  "description": "Telegram-бот YupSoul: приём заявок из Mini App, уведомления",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node healthz.js",
    "dev": "node --watch index.js",
    "run-migration": "node run-migration.js",
    "migrate:all": "node run-all-migrations.js",
    "seed-ideally-tuned": "node seed-ideally-tuned-prompt.js",
    "worker": "node workerGenerate.js",
    "worker:full": "node runFullPipeline.js",
    "worker:loop": "node worker-loop.js",
    "worker:start": "WORKER_INTERVAL_MS=60000 node worker-loop.js",
    "start:all": "./start-all.sh",
    "check-env": "node check-env.js",
    "verify": "node verify-full-cycle.js",
    "ensure-db-url": "node ensure-database-url.js",
    "setup:db": "node run-all-migrations.js && node seed-ideally-tuned-prompt.js",
    "test:apis": "node test-apis.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "grammy": "^1.28.0",
    "pg": "^8.18.0",
    "swisseph": "^0.5.17"
  }
}
